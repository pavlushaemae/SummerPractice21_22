<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_graph"
    app:startDestination="@id/profileFragment">

    <fragment
        android:id="@+id/favouritesFragment"
        android:name="com.example.myapplication.FavouritesFragment"
        android:label="FavouritesFragment"
        tools:layout="@layout/fragment_favourites">
        <action
            android:id="@+id/action_favouritesFragment_to_testForCatFragment"
            app:destination="@id/testForCatFragment" />
    </fragment>
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.example.myapplication.ProfileFragment"
        android:label="ProfileFragment"
        tools:layout="@layout/fragment_profile">
        <action
            android:id="@+id/action_profileFragment_to_testForCatFragment"
            app:destination="@id/testForCatFragment" />
    </fragment>
    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.example.myapplication.SettingsFragment"
        android:label="SettingsFragment"
        tools:layout="@layout/fragment_settings">
        <action
            android:id="@+id/action_settingsFragment_to_testForCatFragment"
            app:destination="@id/testForCatFragment" />
    </fragment>
    <fragment
        android:id="@+id/testForCatFragment"
        android:name="com.example.myapplication.TestForCatFragment"
        android:label="fragment_test_for_cat"
        tools:layout="@layout/fragment_test_for_cat" />
    <fragment
        android:id="@+id/musicFragment"
        android:name="com.example.myapplication.MusicFragment"
        android:label="MusicFragment"
        tools:layout="@layout/fragment_music">
        <action
            android:id="@+id/action_musicFragment_to_testForCatFragment"
            app:destination="@id/testForCatFragment" />
    </fragment>
    <fragment
        android:id="@+id/videosFragment"
        android:name="com.example.myapplication.VideosFragment"
        android:label="VideosFragment"
        tools:layout="@layout/fragment_videos">
        <action
            android:id="@+id/action_videosFragment_to_testForCatFragment"
            app:destination="@id/testForCatFragment" />
    </fragment>
</navigation>